/* Title Template */
tmpl title:
  <h2>Header</h2>;

/* Card Template */
tmpl card:
  <div class="card">
    <-title />
    <p>paragraph</p>
  </div>;

/* Css Stylesheet */
styl card {
  .card {
    width: 300px;
    height: 400px;
  }
}

/* Root Html Template */
tmpl @html:
  <-card />
  <-card />;
